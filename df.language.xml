            </stl-vector>
        </static-array>
        <static-array name='parts' index-enum='language_word_table_index'>
            <stl-vector>
                <enum base-type='int32_t' type-name='part_of_speech'/>
            </stl-vector>
        </static-array>
    </struct-type>

