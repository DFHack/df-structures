        <int16_t name="lake_surface"/>
        <stl-vector name="forces" type-name='int32_t' ref-target='historical_figure' comment="historical figure IDs of force deities associated with the region"/>
        <int32_t name="unk_v47_2"/>
        <int32_t name="mid_x"/>
        <int32_t name="mid_y"/>
