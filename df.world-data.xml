            </stl-vector>
        </static-array>
    </struct-type>

    <enum-type type-name="world_population_type">
        <enum-item name='Animal'/>
        <enum-item name='Vermin'/>
        <enum-item name='Unk2'/>
        <enum-item name='VerminInnumerable'/>
        <enum-item name='ColonyInsect'/>
