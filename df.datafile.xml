<data-definition>
    -- Unused: DataFileIdentifier

    <enum-type type-name='save_type' base-type='int32_t'> bay12: SaveTypeType
        <enum-item name='NONE' value='-1'/>
        <enum-item name='ACTIVE'/>
        <enum-item name='INACTIVE'/>
        <enum-item name='MANUAL'/>
        <enum-item name='AUTO'/>
    </enum-type>

